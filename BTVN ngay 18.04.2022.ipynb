    "#Vẽ N ngôi sao:\n",
    "N = int(input('Nhap so hinh sao: '))\n",
    "import turtle\n",
    "p = turtle.Turtle()\n",
    "for i in range(1,N+1):\n",
    "    j = 0\n",
    "    while j <= 5:\n",
    "        p.fd(50)\n",
    "        p.right(144)\n",
    "        j += 1\n",
    "    p.left(144)\n",
    "turtle.done()"
   ]
  }
